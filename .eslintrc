{
  "parser": "babel-eslint",
  "extends": [
    "airbnb/base",
    "plugin:import/errors",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "no-console": 0,
    "react/jsx-key": 0,
    "semi": 2,
    "import/prefer-default-export": 0
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "react"
  ]
}
